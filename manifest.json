{
    "name": "BookMarks",
    "description": "backup bookmarks while update bookmarks",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "action": {
        "default_title": "My Bookmarks",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}