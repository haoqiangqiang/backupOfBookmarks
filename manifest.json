{
    "name": "BookMarks",
    "description": "backup bookmarks while update bookmarks",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png"
    },
    "storage": {
        "managed_schema": "schema.json"
    }
}